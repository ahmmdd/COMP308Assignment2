<div class="container">
	<div class="row">
		<div class="col-md-offset-3 col-md-6">
			<h1>Business Contact List</h1>
			<ul>
				<% console.log("For Loop!!!"); %>
				<% for(let count = 0; count < contacts.length; count++) { %>
					<li class="list-group-item">
						<%= contacts[count].name %>
					</li>
				<% } %>
			</ul>
		</div>
	</div>


